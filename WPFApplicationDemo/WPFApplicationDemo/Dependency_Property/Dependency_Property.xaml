<Window x:Class="WPFApplicationDemo.Dependency_Property.Dependency_Property"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApplicationDemo.Dependency_Property"
        mc:Ignorable="d"
        Title="DependencyProperty" Height="300" Width="300">
    <StackPanel>
        <TextBox x:Name="t1" BorderBrush="Black" Margin="5"></TextBox>
        <TextBox x:Name="t2" BorderBrush="Black" Margin="5"></TextBox>
        <Button Content="OK" Margin="5" Click="Button_Click"></Button>
    </StackPanel>
</Window>
